      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "prepare": "npm run build"
  },
  "keywords": [
    "cryptic",
    "auth",
    "typescript",
    "client",
    "narangcia"
  ],
  "author": "narangcia <contact@narangcia.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/narangcia-oss/cryptic-auth-clients/Typescript/plain-ts"
  },
  "dependencies": {
    "axios": "1.11.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "28.0.6",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@rollup/plugin-typescript": "12.1.4",
    "rollup": "4.45.1",
    "rollup-plugin-dts": "6.2.1",
    "typescript": "5.8.3",
    "tslib": "^2.8.1"
  }
}
