{
  "name": "@narangcia-oss/cryptic-auth-client-react",
  "version": "0.1.0",
  "description": "React hooks and components for cryptic-auth authentication with seamless OAuth2 integration",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "prepare": "npm run build",
    "dev": "rollup -c --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "cryptic",
    "auth",
    "react",
    "hooks",
    "oauth2",
    "authentication",
    "typescript",
    "narangcia"
  ],
  "author": "narangcia <contact@narangcia.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/narangcia-oss/cryptic-auth-clients/Typescript/react"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@narangcia-oss/cryptic-auth-client-plain-ts": "^0.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "28.0.6",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@rollup/plugin-typescript": "12.1.4",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rollup": "4.45.1",
    "rollup-plugin-dts": "6.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "typescript": "5.8.3",
    "tslib": "^2.8.1"
  }
}
